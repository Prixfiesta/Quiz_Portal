<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Teacher.SubjectCreationController">
   <children>
      <Label alignment="CENTER" layoutX="215.0" layoutY="53.0" prefHeight="27.0" prefWidth="170.0" style="-fx-background-color: black;" text="Create Subject" textFill="#f5f11d">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <JFXTextField fx:id="subjectname" alignment="CENTER" layoutX="125.0" layoutY="111.0" promptText="Name of Subject" />
      <JFXTextField fx:id="subjectcode" alignment="CENTER" layoutX="125.0" layoutY="155.0" promptText="Subject Code" />
      <JFXButton fx:id="createsubject" buttonType="RAISED" layoutX="260.0" layoutY="240.0" onAction="#createSubject" style="-fx-background-color: yellow;" text="Create" />
      <JFXButton fx:id="close" buttonType="RAISED" layoutX="574.0" layoutY="2.0" onAction="#closeClicked" style="-fx-background-color: red;" text="X" />
      <JFXButton fx:id="back" buttonType="RAISED" layoutX="55.0" layoutY="55.0" onAction="#backClicked" style="-fx-background-color: grey;" text="&lt;- Home" textFill="#d21515" />
   </children>
</AnchorPane>
