<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Signup.StudentSignUpController">
    <children>
        <Label alignment="CENTER" layoutX="172.0" layoutY="33.0" prefHeight="17.0" prefWidth="207.0" text="Sign Up as a Student">
            <font>
                <Font size="21.0" />
            </font>
        </Label>
        <JFXTextField fx:id="name" layoutX="195.0" layoutY="86.0" promptText="Name" />
        <JFXTextField fx:id="email" layoutX="195.0" layoutY="135.0" promptText="Email" />
        <JFXTextField fx:id="id" layoutX="195.0" layoutY="188.0" promptText="Your Id" />
        <JFXPasswordField fx:id="password" layoutX="195.0" layoutY="235.0" promptText="Password" />
        <JFXPasswordField fx:id="confpass" layoutX="195.0" layoutY="282.0" promptText="Confirm Password" />
        <JFXButton fx:id="submitbutton" buttonType="RAISED" layoutX="237.0" layoutY="332.0" onAction="#submitClicked" style="-fx-background-color: green;" text="Submit" textFill="#e1ee6e" />
        <JFXButton fx:id="Login" buttonType="RAISED" layoutX="35.0" layoutY="36.0" onAction="#toLoginPage" style="-fx-background-color: yellow;" text="&lt;&lt; Login" />
        <JFXButton fx:id="close" layoutX="584.0" layoutY="2.0" onAction="#closeClicked" style="-fx-background-color: red;" text="X" />
    </children>
</AnchorPane>
